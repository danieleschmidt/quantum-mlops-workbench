# Quantum MLOps Workbench Environment Configuration

# Application Settings
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Database Configuration
DATABASE_URL=postgresql://quantum_user:quantum_pass@localhost:5432/quantum_mlops
REDIS_URL=redis://localhost:6379/0

# Quantum Backend Credentials
# AWS Braket
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1
BRAKET_S3_BUCKET=your-braket-bucket

# IBM Quantum
IBM_QUANTUM_TOKEN=your_ibm_quantum_token
IBM_QUANTUM_INSTANCE=ibm-q/open/main

# IonQ
IONQ_API_KEY=your_ionq_api_key

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_S3_ENDPOINT_URL=http://localhost:9000
MLFLOW_ARTIFACT_ROOT=s3://mlflow-artifacts

# Monitoring and Observability
PROMETHEUS_GATEWAY_URL=http://localhost:9091
GRAFANA_URL=http://localhost:3000
OPENTELEMETRY_ENDPOINT=http://localhost:4317

# Security Settings
SECRET_KEY=your-secret-key-here-change-in-production
JWT_SECRET_KEY=your-jwt-secret-key
API_KEY_SALT=your-api-key-salt

# Model Registry
MODEL_REGISTRY_PATH=./model_registry
EXPERIMENT_TRACKING_PATH=./experiments

# Resource Limits
MAX_QUBITS_SIMULATOR=32
MAX_CONCURRENT_JOBS=10
CACHE_TTL_SECONDS=3600

# Cost Management
DAILY_BUDGET_USD=100.0
MONTHLY_BUDGET_USD=2000.0
COST_ALERT_THRESHOLD=0.8

# Performance Settings
CIRCUIT_CACHE_SIZE=1000
RESULT_CACHE_SIZE=5000
WORKER_THREADS=4